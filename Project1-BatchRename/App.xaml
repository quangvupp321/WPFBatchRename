<Application x:Class="Project1_BatchRename.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:Project1_BatchRename"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <!--Style Button Feature-->
        <Style TargetType="Button" x:Key="button_feature">
            <Setter Property="FontSize" Value="15"></Setter>
            <Setter Property="Padding" Value="5,1,0,1"></Setter>
            <!--<Setter Property="Margin" Value="5,0,5,10"></Setter>-->
            <Setter Property="Background" Value="#BFC9C0"></Setter>
            <Setter Property="FontWeight" Value="Bold"></Setter>
        </Style>
    </Application.Resources>
</Application>
